     /* Yüzen Dekoratif Şekiller - Koyu Yeşil Tonları */
 body::after {
         content: '' !important;
    position: fixed !important;
            top: -50%;
          right: -50%;
     width: 200%;
  height: 200%;
 background: 
     radial-gradient(circle at 5% 10%, rgba(60, 179, 113, 0.18) 0%, transparent 50%),
  radial-gradient(circle at 95% 90%, rgba(46, 139, 87, 0.16) 0%, transparent 50%),
     radial-gradient(circle at 5% 90%, rgba(34, 139, 34, 0.15) 0%, transparent 50%),
         radial-gradient(circle at 95% 10%, rgba(72, 209, 204, 0.17) 0%, transparent 50%),
          radial-gradient(circle at 10% 50%, rgba(50, 205, 50, 0.14) 0%, transparent 50%),
    radial-gradient(circle at 90% 50%, rgba(85, 107, 47, 0.16) 0%, transparent 50%),
      radial-gradient(circle at 3% 30%, rgba(107, 142, 35, 0.13) 0%, transparent 50%),
radial-gradient(circle at 97% 70%, rgba(60, 179, 113, 0.15) 0%, transparent 50%) !important;
    animation: floatBackground 25s ease-in-out infinite !important;
            z-index: -1 !important;
      pointer-events: none;
  }

        @@keyframes floatBackground {
  0%, 100% {
        transform: translate(0, 0) rotate(0deg) scale(1);
}
     25% {
  transform: translate(-8%, -8%) rotate(90deg) scale(1.1);
    }
        50% {
                transform: translate(8%, 8%) rotate(180deg) scale(1);
 }
  75% {
 transform: translate(-6%, 6%) rotate(270deg) scale(1.08);
      }
     }

        /* Hareketli Parçacıklar - Koyu Yeşil Tonları */
body::before {
  content: '' !important;
  position: fixed !important;
   top: 0;
      left: 0;
            width: 100%;
     height: 100%;
    background-image: 
     radial-gradient(circle at 2% 15%, rgba(60, 179, 113, 0.15) 0%, transparent 40%),
  radial-gradient(circle at 98% 85%, rgba(46, 139, 87, 0.14) 0%, transparent 40%),
          radial-gradient(circle at 2% 85%, rgba(34, 139, 34, 0.13) 0%, transparent 40%),
      radial-gradient(circle at 98% 15%, rgba(72, 209, 204, 0.15) 0%, transparent 40%),
   radial-gradient(circle at 8% 50%, rgba(50, 205, 50, 0.12) 0%, transparent 40%),
     radial-gradient(circle at 92% 50%, rgba(85, 107, 47, 0.14) 0%, transparent 40%),
 radial-gradient(circle at 4% 70%, rgba(107, 142, 35, 0.11) 0%, transparent 40%),
     radial-gradient(circle at 96% 30%, rgba(60, 179, 113, 0.13) 0%, transparent 40%) !important;
       background-size: 350% 350%, 320% 320%, 380% 380%, 340% 340%, 360% 360%, 330% 330%, 370% 370%, 350% 350% !important;
   background-position: 0% 0%, 100% 100%, 0% 100%, 100% 0%, 0% 50%, 100% 50%, 0% 70%, 100% 30% !important;
         animation: particleMove 20s ease-in-out infinite !important;
  z-index: -2 !important;
     pointer-events: none;
   }

   @@keyframes particleMove {
            0%, 100% {
   background-position: 0% 0%, 100% 100%, 0% 100%, 100% 0%, 0% 50%, 100% 50%, 0% 70%, 100% 30%;
  }
     25% {
      background-position: 12% 12%, 88% 88%, 8% 92%, 92% 8%, 8% 48%, 92% 52%, 5% 75%, 95% 25%;
   }
50% {
background-position: 25% 25%, 75% 75%, 15% 85%, 85% 15%, 15% 45%, 85% 55%, 10% 80%, 90% 20%;
    }
 75% {
  background-position: 18% 18%, 82% 82%, 12% 88%, 88% 12%, 12% 49%, 88% 51%, 8% 75%, 92% 25%;
 }
        }

